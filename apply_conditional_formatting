# Import the openpyxl library
import openpyxl

# Load the Excel file
wb = openpyxl.load_workbook('your_file.xlsx')

# Select the worksheet
ws = wb['Sheet1']

# Loop through the columns starting from row 2
for col in ws.iter_cols(min_row=2):

    # Create a set to store the unique values
    unique_values = set()

    # Loop through the cells in the column
    for cell in col:

        # Check if the cell value is already in the set
        if cell.value in unique_values:

            # Highlight the cell with yellow color
            cell.fill = openpyxl.styles.PatternFill(start_color="FFFF00", end_color="FFFF00", fill_type="solid")

        else:

            # Add the cell value to the set
            unique_values.add(cell.value)

# Save the Excel file
wb.save('your_file.xlsx')
